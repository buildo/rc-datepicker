{
  "name": "rc-datepicker",
  "version": "1.0.0",
  "description": "DatePicker and DatePickerInput to be used with React and Semantic UI",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start",
    "build": "rm -rf lib && mkdir lib && babel --loose --stage 0 --out-dir lib src",
    "lint": "eslint src",
    "preversion": "npm run lint && npm run test && webpack --config examples/webpack.config.js",
    "prepublish": "npm run build",
    "start": "npm run clean && webpack-dev-server --config examples/webpack.config.js --progress --hot --inline",
    "clean": "rm -rf examples/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/buildo/react-semantic-datepicker.git"
  },
  "keywords": [
    "react",
    "react-component",
    "semantic-ui",
    "semantic",
    "datepicker",
    "calendar",
    "date"
  ],
  "author": "Francesco Cioria <francesco@buildo.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buildo/react-semantic-datepicker/issues"
  },
  "homepage": "https://github.com/buildo/react-semantic-datepicker",
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-eslint": "^3.1.11",
    "babel-loader": "^5.1.3",
    "eslint": "^0.23.0",
    "eslint-loader": "^0.14.0",
    "eslint-plugin-react": "^2.5.2",
    "expect": "^1.6.0",
    "isparta-loader": "^0.2.0",
    "karma": "^0.12.36",
    "karma-bamboo-reporter": "^0.1.0",
    "karma-chrome-launcher": "^0.1.12",
    "karma-coverage": "^0.3.1",
    "karma-mocha": "^0.1.10",
    "karma-webpack": "^1.5.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "react": "^0.13.3",
    "require-dir": "^0.3.0",
    "webpack": "^1.9.12",
    "webpack-dev-server": "^1.9.0"
  },
  "peerDependencies": {
    "react": ">= 0.12.x"
  },
  "dependencies": {
    "classnames": "^1.2.0",
    "lodash": "^3.6.0",
    "moment": "^2.9.0"
  }
}
