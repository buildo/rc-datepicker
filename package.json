{
  "name": "rc-datepicker",
  "version": "5.0.0-beta2",
  "description": "DatePicker and DatePickerInput to be used with React",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start",
    "build": "rm -rf lib && mkdir lib && node-sass src -o lib && babel src -d lib",
    "lint": "eslint src examples/examples.js test",
    "prepublish": "npm run build",
    "preversion": "npm run lint && npm run test && npm run build-examples",
    "build-examples": "npm run clean && webpack --config examples/webpack.config.build.babel.js --progress",
    "start": "npm run clean && webpack-dev-server --config examples/webpack.config.babel.js --progress --hot --inline",
    "clean": "rm -rf examples/build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/buildo/rc-datepicker.git"
  },
  "keywords": [
    "react",
    "react-component",
    "datepicker",
    "calendar",
    "date"
  ],
  "author": "Francesco Cioria <francesco@buildo.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buildo/rc-datepicker/issues"
  },
  "homepage": "https://github.com/buildo/rc-datepicker",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "compression-webpack-plugin": "^0.3.1",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "eslint": "^2.11.1",
    "eslint-config-buildo": "github:buildo/eslint-config",
    "eslint-loader": "^1.3.0",
    "expect": "^1.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^1.7.0",
    "isparta-loader": "^2.0.0",
    "karma": "^0.13.22",
    "karma-bamboo-reporter": "^0.1.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.7.0",
    "react": "^0.14",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^0.14",
    "require-dir": "^0.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "react": "^0.14 || ^0.15",
    "react-dom": "^0.14 || ^0.15"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "revenge": "^0.4.4",
    "tcomb": "^3.2.1",
    "tcomb-react": "^0.9.0"
  }
}
